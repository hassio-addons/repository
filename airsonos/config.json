{
  "name": "AirSonos",
  "version": "3.1.1",
  "slug": "airsonos",
  "description": "AirPlay capabilities for your Sonos (and UPnP) devices.",
  "url": "https://github.com/hassio-addons/addon-airsonos",
  "startup": "services",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "hassio_api": true,
  "host_network": true,
  "map": [
    "config:rw"
  ],
  "options": {
    "port": 49152,
    "latency_rtp": 1000,
    "latency_http": 2000,
    "drift": false
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "address": "str?",
    "port": "port",
    "latency_rtp": "int",
    "latency_http": "int",
    "drift": "bool"
  },
  "image": "ghcr.io/hassio-addons/airsonos/{arch}"
}