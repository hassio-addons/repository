{
  "name": "Tailscale",
  "version": "0.2.0",
  "slug": "tailscale",
  "description": "Zero config VPN for building secure networks",
  "url": "https://github.com/hassio-addons/addon-tailscale",
  "ingress": true,
  "ingress_port": 0,
  "ingress_stream": true,
  "startup": "services",
  "panel_icon": "mdi:vpn",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "init": false,
  "hassio_api": true,
  "privileged": [
    "NET_ADMIN"
  ],
  "devices": [
    "/dev/net/tun"
  ],
  "host_network": true,
  "image": "ghcr.io/hassio-addons/tailscale/{arch}"
}