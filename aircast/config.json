{
  "name": "AirCast",
  "version": "2.2.0",
  "slug": "aircast",
  "description": "AirPlay capabilities for your Chromecast devices.",
  "url": "https://github.com/hassio-addons/addon-aircast",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "host_network": true,
  "map": [
    "config:rw"
  ],
  "options": {
    "address": "",
    "latency_rtp": 0,
    "latency_http": 0,
    "drift": false
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)?",
    "address": "str",
    "latency_rtp": "int",
    "latency_http": "int",
    "drift": "bool"
  },
  "image": "hassioaddons/aircast-{arch}"
}